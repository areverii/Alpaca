function int factorial(int x):
    if(x == 0):
        return 1
    end 
    else:
        return x * factorial (x - 1)
    end
end

function void main(<string> args):
    int f 
    int x 
    x = 10
    f = factorial(x)
    prints("factorial of ")
    printi(x)
    prints(" is ")
    printi(f)
    prints("\n")
end
